<!DOCTYPE html>
<html>
  <head>
    <title>The Gallery</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <%- include('bootstrap') %>
  </head>
  <body>
    <%- include('navbar') %>
    <h1>The Gallery</h1>
    <p>Welcome to The Gallery</p>

    <h2>Bucle artistas</h2>
    <% resultArtist.forEach(artist => { %>
    <a href="/artist/oneArtist/<%= artist.artist_id %>">
      <img
        src="/images/artists/<%= artist.img %>"
        alt=""
        width="200"
      />
    </a>
    <p><%= artist.name %></p>

    <% }) %>


    <%- include('footer') %>

  </body>
</html>
